pipeline{
    agent any
    stages{
        stage('checkout'){
            steps{
                checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Venna12/taxi-booking.git']])
            }
        }
        stage('Build'){
            steps{
                sh "mvn package"
            }
        }
        
    }
}
